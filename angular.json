{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ERP": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/erp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "./node_modules/@syncfusion/ej2-material-theme/styles/material.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap-material-design.js",
              "src/assets/js/modernizr.min.js",
              "src/assets/js/detect.js",
              "src/assets/js/fastclick.js",
              "src/assets/js/jquery.slimscroll.js",
              "src/assets/js/jquery.blockUI.js",
              "src/assets/js/waves.js",
              "src/assets/js/jquery.nicescroll.js",
              "src/assets/js/jquery.scrollTo.min.js",
              "src/assets/plugins/carousel/owl.carousel.min.js",
              "src/assets/plugins/fullcalendar/vanillaCalendar.js",
              "src/assets/plugins/peity/jquery.peity.min.js",
              "src/assets/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js",
              "src/assets/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
              "src/assets/plugins/chartist/js/chartist.min.js",
              "src/assets/plugins/chartist/js/chartist-plugin-tooltip.min.js",
              "src/assets/plugins/metro/MetroJs.min.js",
              "src/assets/plugins/raphael/raphael.min.js",
              "src/assets/pages/dashborad.js",
              "src/assets/js/app.js",
              "src/assets/plugins/morris/morris.min.js",
              "src/assets/plugins/datatables/dataTables.responsive.min.js",
              "src/assets/plugins/datatables/responsive.bootstrap4.min.js",
              "src/assets/pages/datatables.init.js"
              
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ERP:build:production"
            },
            "development": {
              "browserTarget": "ERP:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ERP:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "./node_modules/@syncfusion/ej2-material-theme/styles/material.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "bc7af347-5b68-4085-9b77-7af43836bb15"
  }
}